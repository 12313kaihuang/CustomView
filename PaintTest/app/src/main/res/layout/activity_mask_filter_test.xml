<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.example.painttest.maskfilter.MaskFilterView
        android:id="@+id/mask_filter_view1"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@id/mask_filter_view2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:type="1" />

    <com.example.painttest.maskfilter.MaskFilterView
        android:id="@+id/mask_filter_view2"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/mask_filter_view1"
        app:layout_constraintTop_toTopOf="parent"
        app:type="2" />

    <TextView
        android:id="@+id/view1_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="NORMAL"
        app:layout_constraintEnd_toEndOf="@id/mask_filter_view1"
        app:layout_constraintStart_toStartOf="@id/mask_filter_view1"
        app:layout_constraintTop_toBottomOf="@id/mask_filter_view1" />

    <TextView
        android:id="@+id/view2_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="SOLD"
        app:layout_constraintEnd_toEndOf="@id/mask_filter_view2"
        app:layout_constraintStart_toStartOf="@id/mask_filter_view2"
        app:layout_constraintTop_toBottomOf="@id/mask_filter_view2" />


    <com.example.painttest.maskfilter.MaskFilterView
        android:id="@+id/mask_filter_view3"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@id/mask_filter_view4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/view1_tv"
        app:type="3" />

    <com.example.painttest.maskfilter.MaskFilterView
        android:id="@+id/mask_filter_view4"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/mask_filter_view3"
        app:layout_constraintTop_toBottomOf="@id/view1_tv"
        app:type="4" />

    <TextView
        android:id="@+id/view3_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="INNER"
        app:layout_constraintEnd_toEndOf="@id/mask_filter_view3"
        app:layout_constraintStart_toStartOf="@id/mask_filter_view3"
        app:layout_constraintTop_toBottomOf="@id/mask_filter_view3" />

    <TextView
        android:id="@+id/view4_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="OUTER"
        app:layout_constraintEnd_toEndOf="@id/mask_filter_view4"
        app:layout_constraintStart_toStartOf="@id/mask_filter_view4"
        app:layout_constraintTop_toBottomOf="@id/mask_filter_view4" />

    <TextView
        android:id="@+id/blur_title_tv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="BlurMaskFilter"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/view3_tv" />

    <com.example.painttest.maskfilter.MaskFilterView
        android:id="@+id/mask_filter_view0"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toStartOf="@id/mask_filter_view5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/blur_title_tv"
        app:type="0" />

    <com.example.painttest.maskfilter.MaskFilterView
        android:id="@+id/mask_filter_view5"
        android:layout_width="0dp"
        android:layout_height="120dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/mask_filter_view0"
        app:layout_constraintTop_toBottomOf="@id/blur_title_tv"
        app:type="5" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:text="EmbossMaskFilter"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@id/mask_filter_view0" />

</androidx.constraintlayout.widget.ConstraintLayout>